/* ---- Button ---- */
.zhym-placemarklist-button-ui
{
	text-align: center; 
	font-size: 13px; 
	border-width: 1px;
	border-style: solid;
	border-color: rgb(113, 123, 135);
	box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.4); 
	background: #FFFFFF;
	cursor: pointer;
}

